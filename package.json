{
  "name": "eleventy-plugin-svelte",
  "version": "1.0.2",
  "description": "Use Svelte components in Eleventy.",
  "main": ".eleventy.js",
  "scripts": {
    "dev": "cd example && env ELEVENTY_EXPERIMENTAL=true NODE_ENV=development npx @11ty/eleventy --serve",
